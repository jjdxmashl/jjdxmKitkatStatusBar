<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.dou361.jjdxm_statusbar"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.dou361.jjdxm_statusbar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.dou361.jjdxm_statusbar.OneActivity"
            android:theme="@style/slidAppTheme"/>
        <activity
            android:name="com.dou361.jjdxm_statusbar.TwoActivity"
            android:theme="@style/slidAppTheme"/>
        <activity
            android:name="com.dou361.jjdxm_statusbar.ThreeActivity"
            android:theme="@style/slidAppTheme"/>
    </application>

</manifest>
